<config>
	<menu>
	</menu>
	<acl>
		<resources>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<trustpayments_payment translate="title">
										<title>Trust Payments Payment</title>
										<sort_order>100</sort_order>
									</trustpayments_payment>
								</children>
							</config>
						</children>
					</system>
					<customer>
						<children>
							<manage>
								<children>
									<trustpayments_token translate="title">
		                                <title>Manage Trust Payments Tokens</title>
		                                <sort_order>100</sort_order>
		                            </trustpayments_token>
								</children>
							</manage>
						</children>
					</customer>
				</children>
			</admin>
		</resources>
	</acl>
</config>